{
  "name": "nest-micro",
  "description": "nest-micro description",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">= 12.13.0"
  },
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build --tsc",
    "prestart": "yarn run build",
    "start": "cross-env NODE_ENV=production pm2 start process.json",
    "format": "prettier \"**/*.ts\" --write && git status",
    "lint": "eslint \"src/**/*.ts\" --fix --ignore-pattern \"**/*.spec.ts\"",
    "i18next": "node src/lib/i18next/i18next.converter.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit --detectOpenHandles",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "compodoc": "compodoc -p tsconfig.json -s",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.{ts,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.16",
    "@nestjs/common": "8.0.6",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "8.0.6",
    "@nestjs/graphql": "^10.0.16",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "5.0.9",
    "apollo-server-express": "^3.9.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "debug": "4.3.2",
    "graphql-request": "3.5.0",
    "helmet": "4.6.0",
    "lodash": "4.17.21",
    "mime-types": "2.1.32",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "morgan": "1.10.0",
    "path": "^0.12.7",
    "pug": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "shelljs": "0.8.4",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-angular": "13.1.0",
    "@commitlint/config-conventional": "11.0.0",
    "@nestjs/cli": "8.1.1",
    "@nestjs/testing": "8.0.6",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.172",
    "@types/mime-types": "2.1.1",
    "@types/morgan": "1.9.3",
    "@types/multer": "1.4.7",
    "@types/pug": "2.0.5",
    "@types/shelljs": "0.8.9",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "graphql": "^16.5.0",
    "husky": "^7.0.0",
    "jest": "27.2.0",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "typescript": "4.4.3"
  }
}
